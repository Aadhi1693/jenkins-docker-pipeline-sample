pipeline  { 
	agent any  

stage('Build') {
		steps {
			echo ' Building a project....'
			sh 'echo Building stage'
		}
	}

stage('Test') {
		steps {
			echo ' Testing the project...'
			sh 'echo Testing stage'
		}
	}

stage('Deploy') {
		steps {
			echo 'Deploying the project..'
			sh 'echo Deployment stage'
		}
	}
}
}
